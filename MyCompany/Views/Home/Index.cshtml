@{
    ViewBag.Title = "Home Page";
}
@section Scripts {
<script src="~/Scripts/Form/HomeIndex.js"></script>
    <script>
        remove = function ()
        {
            $("#modaldelete").modal();
        }

    </script>
}

<div ng-app="MyApp">
    <div ng-controller="IndexDataCtrl">
        <div class="row" style="margin-top:30px">
            <div class="col-lg-12">
                <div ng-repeat="m in IndexData" class="col-sm-6 col-md-3">
                    <a href="#" ng-click="goToCampaign()">
                        <div align="center" class="thumbnail">
                            <div ng-if="m.val==1">
                                <img src="~/Content/Icon/HNI.png" class="home-image" />
                            </div>
                            <div ng-if="m.val==2">
                                <img src="~/Content/Icon/computer_store.png" class="home-image" />
                            </div>
                            <div ng-if="m.val==3">
                                <img src="~/Content/Icon/laundry_shop.png" class="home-image" />
                            </div>
                            <div ng-if="m.val==4">
                                <img src="~/Content/Icon/car_wash.png" class="home-image" />
                            </div>  
                            <div class="caption">
                                <h4>{{m.label}}</h4>
                            </div>                          
                        </div>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>
